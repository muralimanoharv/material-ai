{
  "packages": {
    ".": {
      "release-type": "python",
      "extra-files": [
        {
          "type": "toml",
          "path": "pyproject.toml",
          "jsonpath": "project.version"
        },
        {
          "type": "python",
          "path": "src/material_ai/__init__.py",
          "version_name": "__version__"
        },
        {
          "type": "generic",
          "path": "scripts/create_project.sh",
          "regex": "DEFAULT_PROJECT_VERSION=(.+)"
        },
        {
          "type": "python",
          "path": "scripts/__init__.py",
          "version_name": "__version__"
        }
      ]
    }
  }
}