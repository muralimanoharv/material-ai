{
  "packages": {
    ".": {
      "release-type": "simple",
      "extra-files": [
        {
          "type": "generic",
          "path": "pyproject.toml",
          "regex": "version = \"(.+)\""
        },
        {
          "type": "generic",
          "path": "src/material_ai/__init__.py",
          "regex": "__version__ = \"(.+)\""
        },
        {
          "type": "generic",
          "path": "scripts/create_project.sh",
          "regex": "DEFAULT_PROJECT_VERSION=(.+)"
        }
      ]
    }
  }
}